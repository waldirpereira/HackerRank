using System;
class Solution {
  public static void Main(){
    Console.WriteLine("5");
    Console.WriteLine("4 3");
    Console.WriteLine("-1 0 2 3"); //YES
    Console.WriteLine("5 4");
    Console.WriteLine("-2 -3 0 1 -1"); //NO
    Console.WriteLine("8 4");
    Console.WriteLine("2 5 0 -2 1 3 5 -1"); //YES
    Console.WriteLine("6 1");
    Console.WriteLine("-1 0 1 2 -2 3"); //NO
    Console.WriteLine("7 5");
    Console.WriteLine("-1 0 2 4 5 -2 -4"); //YES
  }
}
